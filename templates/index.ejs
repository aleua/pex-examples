<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Pex examples</title>
  <style type="text/css">
  .thumb {
    float: left;
    margin-right: 1em;
  }
  img + p {
    margin-top: 0;
    padding-top: 0;
  }
  </style>
</head>
<body>
  <h1>Pex examples</h1>
  <% examples.forEach(function(example) { %>
    <div class="thumb">
      <a href="examples/<%= example %>">
        <img src="examples/<%= example %>/thumb.jpg" width="300"/>
        <p><%= example %></p>
      </a>
    </div>
  <% }) %>
</body>
</html>
